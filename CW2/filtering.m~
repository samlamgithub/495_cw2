function [ alpha_zt ] = Filtering(nums, T, pi, A, E, Y, t)

[Num_state, Num_obser] = size(E);
Seq_Len = size(Y, 2);

alpha = zeros(Num_state, t);
for k = 1:Num_state
    alpha(k, 1) = pi(k) * E(k, Y(1));
end
alpha = alpha./sum(alpha);
    
for k = 2:t
    alpha(:, k) = E()
    
    
end
    
    
end